{
  "name": "ini-es",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@10.29.2",
  "description": "ESM and TypeScript rewrite of [npm/ini](https://github.com/npm/ini)",
  "keywords": [
    "decode",
    "encode",
    "ini",
    "parse",
    "stringify"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/ini-es#readme",
  "repository": "ntnyq/ini-es",
  "bugs": {
    "url": "https://github.com/ntnyq/ini-es/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./fs": {
      "types": "./dist/fs.d.mts",
      "default": "./dist/fs.mjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.9.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^25.2.2",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,vue,yml,yaml,toml,json}": "eslint --fix"
  }
}
